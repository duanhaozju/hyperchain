contract mortal {
     address owner;

     function mortal() {
         owner = msg.sender;
     }

     function kill() {
         if (msg.sender == owner)
             selfdestruct(owner);
     }
 }


 contract greeter is mortal {
     bytes32 greeting;
     uint32 sum;
     function greeter(bytes32 _greeting) public {
         greeting = _greeting;
     }

     function greet() constant returns (bytes32) {
         return greeting;
     }

     function add(uint32 num1,uint32 num2) {
        sum = sum+num1+num2;
     }
 }

